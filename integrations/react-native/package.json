    "url": "https://github.com/bunnyvault/react-native-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/bunnyvault/react-native-sdk/issues"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.60.0"
  },
  "dependencies": {
    "react-native-video": "^5.2.1",
    "react-native-webview": "^13.6.4",
    "@react-native-async-storage/async-storage": "^1.19.5",
