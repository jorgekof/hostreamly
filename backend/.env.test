# Environment
NODE_ENV=development
PORT=5000

# Database (SQLite for testing)
DB_HOST=localhost
DB_PORT=3306
DB_NAME=./test.db
DB_USER=
DB_PASSWORD=
DB_DIALECT=sqlite
DB_LOGGING=true
DB_POOL_MAX=5
DB_POOL_MIN=0
DB_POOL_ACQUIRE=30000
DB_POOL_IDLE=10000

# Redis (disabled for testing)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_KEY_PREFIX=bunnyvault:
REDIS_SESSION_TTL=86400

# JWT Configuration
JWT_SECRET=test_jwt_secret_for_development_only
JWT_REFRESH_SECRET=test_refresh_secret_for_development_only
JWT_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# Session Configuration
SESSION_SECRET=test_session_secret_for_development_only
SESSION_NAME=bunnyvault.sid
SESSION_MAX_AGE=86400000
SESSION_SECURE=false
SESSION_HTTP_ONLY=true
SESSION_SAME_SITE=lax

# Bunny.net Configuration (test values)
BUNNY_API_KEY=test_api_key
BUNNY_LIBRARY_ID=test_library_id
BUNNY_CDN_HOSTNAME=test.b-cdn.net
BUNNY_CDN_PULL_ZONE=test_pull_zone
BUNNY_STORAGE_ZONE=test_storage_zone
BUNNY_STORAGE_PASSWORD=test_storage_password
BUNNY_STORAGE_HOSTNAME=storage.bunnycdn.com
BUNNY_WEBHOOK_SECRET=test_webhook_secret
BUNNY_STREAM_WEBHOOK_SECRET=test_stream_webhook_secret

# Agora Configuration (test values)
AGORA_APP_ID=test_app_id
AGORA_CERTIFICATE=test_certificate
AGORA_CUSTOMER_ID=test_customer_id
AGORA_CUSTOMER_SECRET=test_customer_secret